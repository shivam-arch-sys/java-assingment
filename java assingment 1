import java.util.Scanner;

public class SumLastDigits {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num1 = sc.nextInt();
        int num2 = sc.nextInt();
        int lastDigit1 = Math.abs(num1 % 10);
        int sum = lastDigit1 + lastDigit2;
        System.out.println("Sum of last digits: " + sum);
    }
}
6-
import java.util.Scanner;

public class MultipleCheck {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        int M = sc.nextInt();

        if (M == 0) {
            System.out.println("M cannot be zero!");
        } else if (N % M == 0) {
            System.out.println(N + " is an exact multiple of " + M);
        } else {
            System.out.println(N + " is NOT an exact multiple of " + M);
        }
    }
}
