public class SumOddDigits {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = Math.abs(sc.nextInt());
        int sumOdd = 0;
        while (num > 0) {
            int digit = num % 10;
            if (digit % 2 != 0) sumOdd += digit;
            num /= 10;
        }

        System.out.println("Sum of odd digits: " + sumOdd);
    }
}
22-
import java.util.Scanner;

public class SumDigitsOpt {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter a number: ");
        int num = Math.abs(sc.nextInt());
        char choice = sc.next().toUpperCase().charAt(0);
        int sum = 0;
        while (num > 0) {
            int digit = num % 10;
            if ((choice == 'E' && digit % 2 == 0) || (choice == 'O' && digit % 2 != 0)) {
                sum += digit;
            }
            num /= 10;
        }

        System.out.println("Sum of chosen digits: " + sum);
    }
}
