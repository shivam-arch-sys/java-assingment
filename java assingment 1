import java.util.Scanner;

public class CountEvenNumbers {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int countEven = 0;
        for (int i = 1; i <= 5; i++) {
            int num = sc.nextInt();
            if (num % 2 == 0) {
                countEven++;
            }
        }

        System.out.println("Number of even numbers: " + countEven);
    }
}
