import java.util.*;

public class CreatePINUsing {
    public static void main(String[] args) {
        Scanner sc =new Scanner(System.in);
        int x=sc.nextInt();
        int y=sc.nextInt();
        int z=sc.nextInt();
       
        int r=0 ;
        int k=0;
        int l=9999999;
        while(x!=0){
            r=x%10;
            if(r>k){
                k=r;
            }if(r<l){
                l=r;
            }
            x=x/10;
            
        }
        int a,d=0;
        int c=999999999;
        int b=0;
        int e=0;
        int f=999999999;
        while(y!=0){
            a=y%10;
            if(a>b){
                b=a;
            }if(a<c){
                c=a;
            }
            y=y/10;
        }while(z!=0){
            d=z%10;
            if(d>e){
                e=d;
            }if(d<f){
                f=d;
            }
            z=z/10;      
        }
        
        System.out.print(l);
        System.out.print(c);
        System.out.print(f);
        System.out.print(k);
        System.out.print(b);
        System.out.print(e);
    }
    
}
        








